<?xml version="1.0" encoding="utf-8"?>
<com.qmuiteam.qmui.widget.QMUIWindowInsetLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <com.feiling.dasong.ui.function.statistics.WorkHourParamsView
            android:id="@+id/workHourParamsView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="?attr/qmui_topbar_height"
            android:background="@color/qmui_config_color_white"
            android:fitsSystemWindows="true"
            app:qmui_skin_background="attr/app_skin_common_background" />

    <com.qmuiteam.qmui.widget.QMUITopBarLayout xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/topbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fitsSystemWindows="true" />


</com.qmuiteam.qmui.widget.QMUIWindowInsetLayout>